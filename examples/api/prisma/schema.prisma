generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Game {
  id        String   @id @default(cuid())
  name      String   @unique
  summary   String?
  updatedAt DateTime @updatedAt
  createdAt Decimal  @default(now())
}
